{
    "listContent": {
        "photoCard": {
            "sap.app": {
                "id": "idPhotoCard",
                "i18n": "i18n/i18n.properties"
            },
            "sap.card": {
                "type": "Object",
                "data": {
                    "request": {
                        "url": "dataSources/french-cv.json"
                    }
                },
                "headerPosition": "Top",
                "header": {
                    "icon": {
                        "src": "sap-icon://camera"
                    },
                    "title": "{i18n>workSince} {= format.dateTime(${/start_experience_date}, {relative: true, relativeStyle: 'wide', relativeScale: 'auto' }) }",
                    "subTitle": "{i18n>born} {= format.dateTime(${/birth_date}, {relative: true, relativeStyle: 'wide', relativeScale: 'auto' }) }"
                },
                "content": {
                    "groups": [
                        {
                            "items": [
                                {
                                    "type": "Image",
                                    "src": "Pictures/Moi.jpg"
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "infoCard": {
            "sap.app": {
                "id": "idInfoCard",
                "i18n": "./i18n/i18n.properties"
            },
            "sap.card": {
                "type": "Object",
                "data": {
                    "request": {
                        "url": "dataSources/french-cv.json"
                    }
                },
                "header": {
                    "icon": {
                        "src": "sap-icon://business-card"
                    },
                    "title": "{{contactDetails}}"
                },
                "content": {
                    "groups": [
                        {
                            "title": "{/name}",
                            "items": [
                                {
                                    "label": "{{phone}}",
                                    "value": "{/contact/phone}",
                                    "actions": [
                                        {
                                            "type": "Navigation",
                                            "parameters": {
                                                "url": "tel:{/contact/phone}"
                                            }
                                        }
                                    ]
                                },
                                {
                                    "label": "{{email}}",
                                    "value": "{/contact/email}",
                                    "actions": [
                                        {
                                            "type": "Navigation",
                                            "parameters": {
                                                "url": "mailto:{/contact/email}"
                                            }
                                        }
                                    ]
                                },
                                {
                                    "label": "{{address}}",
                                    "value": "{/contact/address}"
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "resumeCard": {
            "sap.app": {
                "id": "idResumeCard",
                "i18n": "./i18n/i18n.properties"
            },
            "sap.card": {
                "type": "Object",
                "data": {
                    "request": {
                        "url": "dataSources/french-cv.json"
                    }
                },
                "header": {
                    "icon": {
                        "src": "sap-icon://shortcut"
                    },
                    "title": "{/title}"
                },
                "content": {
                    "groups": [
                        {
                            "title": "{{summary}}",
                            "items": [
                                {
                                    "value": "{/professional_summary}"
                                }
                            ]
                        }
                    ]
                }
            }
        },
        "keySkillsECC": {
            "sap.app": {
                "id": "idKeySkills",
                "i18n": "i18n/i18n.properties"
            },
            "sap.card": {
                "type": "List",
                "data": {
                    "request": {
                        "url": "dataSources/french-cv.json"
                    }
                },
                "header": {
                    "icon": {
                        "src": "Pictures/SAP ECC.jpg"
                    },
                    "title": "ECC",
                    "subTitle": "{i18n>keySkills}"
                },
                "content": {
                    "maxItems": 10,
                    "data": {
                        "path": "/key_skills/SAP_ECC/"
                    },
                    "item": {
                        "title": "{title}",
                        "description": "{description}",
                        "icon": {
                            "src": "sap-icon://favorite"
                        }
                    }
                }
            }
        },
        "keySkillsS4": {
            "sap.app": {
                "id": "idKeySkills",
                "i18n": "i18n/i18n.properties"
            },
            "sap.card": {
                "type": "List",
                "data": {
                    "request": {
                        "url": "dataSources/french-cv.json"
                    }
                },
                "header": {
                    "icon": {
                        "src": "Pictures/SAP S4HANA.png"
                    },
                    "title": "S/4HANA",
                    "subTitle": "{i18n>keySkills}"
                },
                "content": {
                    "maxItems": 10,
                    "data": {
                        "path": "/key_skills/S4_HANA/"
                    },
                    "item": {
                        "title": "{title}",
                        "description": "{description}",
                        "icon": {
                            "src": "sap-icon://favorite"
                        }
                    }
                }
            }
        },
        "keySkillsBTP": {
            "sap.app": {
                "id": "idKeySkills",
                "i18n": "i18n/i18n.properties"
            },
            "sap.card": {
                "type": "List",
                "data": {
                    "request": {
                        "url": "dataSources/french-cv.json"
                    }
                },
                "header": {
                    "icon": {
                        "src": "Pictures/SAP BTP.jpeg"
                    },
                    "title": "SAP Business Technology Platform",
                    "subTitle": "{i18n>keySkills}"
                },
                "content": {
                    "maxItems": 10,
                    "data": {
                        "path": "/key_skills/SAP_Business_Technology_Platform/"
                    },
                    "item": {
                        "title": "{title}",
                        "description": "{description}",
                        "icon": {
                            "src": "sap-icon://favorite"
                        }
                    }
                }
            }
        }
    }
}